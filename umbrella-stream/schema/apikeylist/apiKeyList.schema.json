{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "ApiKeyList",
  "title": "ApiKeyList",
  "description": "Sample JSON schema; for utilities to edit this schema see https://json-schema.org/implementations.html",
  "type": "object",
  "required": [
    "apiKeyDescriptions"
  ],
  "properties": {
    "apiKeyDescriptions": {
      "type": "array",
      "items": {
        "title": "ApiKeyDescription",
        "type": "object",
        "required": [
          "name",
          "enabled",
          "isAdmin"
        ],
        "properties": {
          "name": {
            "type": "string"
          },
          "enabled": {
            "type": "boolean"
          },
          "isAdmin": {
            "type": "boolean"
          },
          "allowedEventTypes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
